<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@styleSheet.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jain_inventory.view.OrdersController">
   <children>
      <Label focusTraversable="false" layoutX="185.0" layoutY="167.0" text="Jain Textiles">
         <font>
            <Font size="44.0" />
         </font>
      </Label>
      <Button fx:id="back" focusTraversable="false" layoutX="516.0" layoutY="360.0" onAction="#back" prefHeight="25.0" prefWidth="69.0" styleClass="navigation" text="Back" />
      <Button fx:id="salesOrders" focusTraversable="false" mnemonicParsing="false" onAction="#openSalesOrders" prefHeight="25.0" prefWidth="150.0" styleClass="navigation-tab" text="Sales Orders" />
      <Button fx:id="purchaseOrders" focusTraversable="false" layoutX="150.0" mnemonicParsing="false" onAction="#openPurchaseOrders" prefHeight="25.0" prefWidth="150.0" styleClass="navigation-tab" text="Purchase Orders" />
      <Button fx:id="customers" focusTraversable="false" layoutX="300.0" mnemonicParsing="false" onAction="#openCustomers" prefHeight="25.0" prefWidth="150.0" styleClass="navigation-tab" text="Customers" />
      <Button fx:id="suppliers" focusTraversable="false" layoutX="450.0" mnemonicParsing="false" onAction="#openSuppliers" prefHeight="25.0" prefWidth="150.0" styleClass="navigation-tab" text="Suppliers" />
